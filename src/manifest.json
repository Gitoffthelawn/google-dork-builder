{
  "manifest_version": 2,
  "name": "Google Dork Builder",
  "version": "0.3.1",
  "description": "Easily create your queries with google operators from the sidebar or by selecting text from the context menu.",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "16": "icons/logo16.png",
      "32": "icons/logo32.png"
    }
  },
  "sidebar_action": {
    "default_title": "Google Dork Builder",
    "default_panel": "sidebar.html",
    "default_icon": {
      "16": "icons/logo16.png",
      "32": "icons/logo32.png"
    }
  },
  "permissions": [
    "contextMenus",
    "storage"
  ]
}
